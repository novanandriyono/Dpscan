{
    "name": "novanandriyono/dpscan",
    "description": "Just a directory scanner.",
    "homepage": "https://github.com/novanandriyono/dpscan",
    "license": "MIT",
    "keywords": [
        "laravel","scandir","dpscan","recursive","dir","directory"
    ],
    "authors": [
        {
            "name": "Novan Andriyono",
            "email": "evelynradtya@gmail.com",
            "homepage": "https://github.com/novanandriyono",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "evelynradtya@gmail.com",
        "issues": "https://github.com/novanandriyono/dpscan/issues",
        "source": "https://github.com/novanandriyono/dpscan"
    },
    "require": {
        "php": ">=5.6.4",
        "illuminate/container": "5.6.*",
        "illuminate/cache": "5.6.*",
        "illuminate/filesystem": "5.6.*",
        "symfony/process":"~4.0",
        "symfony/var-dumper":"~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Dpscan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dpscan\\DpscanServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
