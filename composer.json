{
    "name": "novanandriyono/dpscan",
    "description": "Just a directory scanner.",
    "homepage": "https://github.com/novanandriyono/dpscan",
    "license": "MIT",
    "keywords": [
        "laravel","scandir","dpscan","recursive","dir","directory"
    ],
    "authors": [
        {
            "name": "Novan Andriyono",
            "email": "evelynradtya@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Dpscan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dpscan\\DpscanServiceProvider"
            ],
            "aliases": {
                "Dpscan": "Dpscan\\Support\\Facades\\Dpscan"
            },
            "branch-alias": {
                "dev-master": "1.0.4-dev"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
